	<system.serviceModel>
		<services>
			<service name ="MathServiceLibrary.MathService">

				<endpoint address="http://localhost:4444/MathService"
						  binding="basicHttpBinding"
						  bindingConfiguration=""
						  name="MathService_BasicHttp"
